{
  "name": "tiny-api",
  "version": "1.0.0",
  "description": "TinyUrl API ",
  "main": "server.js",
  "scripts": {
    "start": "export NODE_ENV='production' && nodemon --exec npm run babel-node -- server.js",
    "dev": "export NODE_ENV='development' && npm run lint && nodemon --exec npm run babel-node -- server.js",
    "babel-node": "babel-node --presets=es2015",
    "lint": "eslint . --ext .js --ext .jsx --ignore-path .gitignore --ignore-pattern test/*js --cache",
    "test": "export NODE_ENV='test' && ./node_modules/.bin/mocha --reporter spec"
  },
  "keywords": [
    "Tiny"
  ],
  "author": "Sebasti√°n Osorio <sir@galleto.xyz>",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.15.2",
    "cors": "^2.8.1",
    "express": "^4.14.0",
    "lodash": "^4.17.4",
    "mongoose": "^4.7.7",
    "mongoose-type-url": "^1.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.18.0",
    "chai": "^3.5.0",
    "eslint": "^3.13.1",
    "http-status-codes": "^1.0.6",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "supertest": "^2.0.1"
  }
}
